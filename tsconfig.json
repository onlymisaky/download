{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist/typings",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
  ]
}
